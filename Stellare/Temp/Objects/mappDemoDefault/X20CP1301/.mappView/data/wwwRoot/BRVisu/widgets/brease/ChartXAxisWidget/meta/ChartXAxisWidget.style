<?xml version="1.0" encoding="utf-8"?>
<StyleInformation name="widgets.brease.ChartXAxisWidget" xmlns="http://www.br-automation.com/iat2014/widgetStyles" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
	<StyleProperties>

    <StyleProperty name="axisColor" category="Appearance" type="Color" default="#000000">
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;:not(.disabled) path" attribute="stroke"></StyleElement>
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;:not(.disabled) line" attribute="stroke"></StyleElement>
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;:not(.disabled) text" attribute="fill"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget:not(.disabled) path" attribute="stroke"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget:not(.disabled) line" attribute="stroke"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget:not(.disabled) text" attribute="fill"></StyleElement>
      <Description>
        Color of the axis and its labels
      </Description>
    </StyleProperty>

    <StyleProperty name="disabledAxisColor" category="Appearance" type="Color" default="#EEEEEE">
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;.disabled path" attribute="stroke"></StyleElement>
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;.disabled line" attribute="stroke"></StyleElement>
      <StyleElement idsuffix="_breaseChartXAxis" selector="&amp;.disabled text" attribute="fill"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget.disabled path" attribute="stroke"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget.disabled line" attribute="stroke"></StyleElement>
      <StyleElement selector="&amp;.editMode.breaseChartXAxisWidget.disabled text" attribute="fill"></StyleElement>
      <Description>
        Color of the axis and its labels when it is disabled
      </Description>
    </StyleProperty>

    <StyleProperty name="height" type="Size" not_styleable="true" category="Layout" default="50">
      <StyleElement attribute="@include elemHeight($value)"></StyleElement>
      <StyleElement attribute="@include elemFlexBasis($value)"></StyleElement>
      <Description>Outer height of widget</Description>
    </StyleProperty>
    
    <StyleProperty name="width" type="Size" not_styleable="true" hide="true">
      <StyleElement attribute="@include elemWidth($value)"></StyleElement>
    </StyleProperty>
    
    <StyleProperty name="top" type="Integer" not_styleable="true" hide="true">
      <StyleElement attribute="@include elemTop($value)"></StyleElement>
    </StyleProperty>
    <StyleProperty name="left" type="Integer" not_styleable="true" hide="true">
      <StyleElement attribute="@include elemLeft($value)"></StyleElement>
    </StyleProperty>
    
	</StyleProperties>
</StyleInformation>
	
	

