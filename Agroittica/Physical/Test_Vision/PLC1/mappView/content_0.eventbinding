<?xml version="1.0" encoding="utf-8"?>
<EventBindingSet id="content_0_eventbinding" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2" xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Bindings>
    <EventBinding id="EventBinding_1">
      <Source xsi:type="opcUa.Event" refId="::AsGlobalPV:Status" event="ValueChanged" />
      <EventHandler condition="newValue = 0">
        <Action>
          <Target xsi:type="widgets.brease.Label.Action" contentRefId="content_0" widgetRefId="Label1">
            <Method xsi:type="widgets.brease.Label.Action.SetText" text="STOP" />
          </Target>
        </Action>
      </EventHandler>
      <EventHandler condition="newValue = 10">
        <Action>
          <Target xsi:type="widgets.brease.Label.Action" contentRefId="content_0" widgetRefId="Label1">
            <Method xsi:type="widgets.brease.Label.Action.SetText" text="AUTO" />
          </Target>
        </Action>
      </EventHandler>
      <EventHandler condition="newValue = 20">
        <Action>
          <Target xsi:type="widgets.brease.Label.Action" contentRefId="content_0" widgetRefId="Label1">
            <Method xsi:type="widgets.brease.Label.Action.SetText" text="MANUAL" />
          </Target>
        </Action>
      </EventHandler>
      <EventHandler condition="newValue = 30">
        <Action>
          <Target xsi:type="widgets.brease.Label.Action" contentRefId="content_0" widgetRefId="Label1">
            <Method xsi:type="widgets.brease.Label.Action.SetText" text="ERROR" />
          </Target>
