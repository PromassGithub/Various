<?xml version="1.0" encoding="utf-8"?>
<Area xmlns="http://www.br-automation.com/iat2014/widget" >
  <StyleProperties>
    <StyleProperty name="width" type="Size" not_styleable="true" category="Property" default="800">
      <StyleElement attribute="@include elemWidth($value)"></StyleElement>
      <Description>outer width of widget</Description>
    </StyleProperty>
    <StyleProperty name="height" type="Size" not_styleable="true" category="Property" default="600">
      <StyleElement attribute="@include elemHeight($value)"></StyleElement>
      <Description>outer height of widget</Description>
    </StyleProperty>
    <StyleProperty name="top" type="Integer" not_styleable="true" default="0" category="Layout">
      <StyleElement attribute="@include elemTop($value)"/>
      <Description>absolute position measured from top boundary of parent container</Description>
    </StyleProperty>
    <StyleProperty name="left" type="Integer" not_styleable="true" default="0" category="Layout" >
      <StyleElement attribute="@include elemLeft($value)"/>
      <Description>absolute position measured from left boundary of parent container</Description>
    </StyleProperty>
  </StyleProperties>
</Area>
