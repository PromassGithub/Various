<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
	  xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:cat="http://schemas.microsoft.com/xsd/catalog" >
  <xsl:output method="html" doctype-system="about:legacy-compat" encoding="UTF-8" indent="yes"/>
  <xsl:strip-space elements="*"></xsl:strip-space>


  <xsl:template match="@*|node()">
    <xsl:copy>
      <xsl:apply-templates select="@*|node()"/>
    </xsl:copy>
  </xsl:template>


  <xsl:template match="/xs:schema">
    <html lang="en">
      <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title> Types Documentation</title>
        <!-- generated by widgetTypes2Documentation.xslt -->
        <!-- Styles -->
        <link rel="stylesheet" href="../../../../../../Help/styles.css" />
        <link rel="stylesheet" href="../../styles/mappView.css" />
        <script src="highlighter.js"></script>
      </head>
      <body onload="DomcatInitialize();highlight();">
        <script src="../../../../../../AS/Glossary/glossary.js" type="text/javascript"></script>
        <script src="../../../../../../AS/Domcat/Domcat.js" type="text/javascript"></script>
        <div class="container">
          <h1>Type Documentation</h1>
          <div>
            <div>
              <h2>Properties</h2>
            </div>
            <table class="parameter_tab">
              <thead>
                <tr>
                  <th class="parameter_tab">Name</th>
                  <th class="parameter_tab">Description</th>
                </tr>
              </thead>
              <tbody>
                <xsl:apply-templates select="xs:simpleType|xs:complexType">
                  <xsl:sort select="./@name"
                    lang="en"
                    data-type="text"
                    order="ascending"
                    case-order="lower-first"/>
                </xsl:apply-templates>

              </tbody>
            </table>
          </div>
        </div>
      </body>
    </html>
  </xsl:template>

  <xsl:template match="xs:simpleType">
    <xsl:element name="tr">
      <xsl:attribute name="data-id">
        <xsl:value-of select="./@name"/>
      </xsl:attribute>
      <td class="parameter_tab">
        <a>
          <xsl:attribute name="name">
            <xsl:value-of select="./@name"/>
          </xsl:attribute>
        </a>
        <xsl:value-of select="./@name"/>
      </td>
      <td class="parameter_tab">
        <xsl:apply-templates select="xs:annotation/xs:documentation"></xsl:apply-templates>
      </td>
    </xsl:element>
  </xsl:template>

  <xsl:template match="xs:complexType">
    <xsl:element name="tr">
      <xsl:attribute name="data-id">
        <xsl:value-of select="./@name"/>
      </xsl:attribute>
      <td class="parameter_tab">
        <a>
          <xsl:attribute name="name">
            <xsl:value-of select="./@name"/>
          </xsl:attribute>
        </a>
        <xsl:value-of select="./@name"/>
      </td>
      <td class="parameter_tab">
        <xsl:apply-templates select="xs:annotation/xs:documentation"></xsl:apply-templates>
      </td>
    </xsl:element>
  </xsl:template>

  <xsl:template match="xs:annotation/xs:documentation">
    <xsl:apply-templates select="@*|node()"/>
  </xsl:template>



</xsl:stylesheet>